{
  "name": "@crossmarkio/sdk",
  "version": "0.3.5-beta",
  "repository": "https://github.com/crossmarkio/sdk.git",
  "author": "interc0der <intercoder@crossmark.io>",
  "license": "MIT",
  "keywords": [
    "sdk",
    "crossmark",
    "XRPL",
    "wallet"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/pack/cjs/index.js",
  "types": "dist/pack/cjs/index.d.ts",
  "unpkg": "dist/pack/umd/index.js",
  "jsdelivr": "dist/pack/umd/index.js",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "prebuild": "rm -rf ./dist && rm -rf ./build",
    "postbuild": "yarn test",
    "build": "yarn prebuild && yarn c && yarn postbuild",
    "c": "tsc && tsc-alias && yarn dist",
    "dist": "yarn webpack && yarn rollup",
    "rollup": "rollup -c ./config/rollup.config.mjs",
    "webpack": "npx webpack -c ./config/webpack.config.js",
    "lint": "eslint src --report-unused-disable-directives",
    "format": "prettier --write --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-path ../.gitignore",
    "format:check": "prettier --check --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-path ../.gitignore",
    "test": "jest -all",
    "prepare": "yarn ts-node ./scripts/prepare",
    "revert": "yarn ts-node ./scripts/revert",
    "pub:beta": "yarn build && npm publish --tag beta",
    "pub:next": "yarn build && npm publish --tag next",
    "pub:release": "yarn build && yarn prepare && npm publish --access public && yarn revert"
  },
  "devDependencies": {
    "@types/fs-extra": "^11",
    "@types/jest": "^29.5.2",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "fs-extra": "^11.1.1",
    "https-browserify": "^1.0.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "rollup": "^3.28.1",
    "rollup-plugin-dts": "^6.1.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6",
    "url": "^0.11.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@crossmarkio/typings": "0.0.1-beta",
    "@crossmarkio/utils": "0.0.2-beta",
    "events": "^3.3.0"
  }
}