{
  "name": "@crossmarkio/typings",
  "version": "0.0.1-beta",
  "author": "interc0der <intercoder@crossmark.io>",
  "license": "MIT",
  "keywords": [
    "sdk",
    "crossmark",
    "XRPL",
    "wallet"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "prebuild": "rm -rf ./dist",
    "postbuild": "yarn test",
    "build": "yarn prebuild && yarn c && yarn postbuild",
    "c": "rm -rf ./dist && tsc && tsc-alias",
    "lint": "eslint src --report-unused-disable-directives",
    "format": "prettier --write --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-path ../.gitignore",
    "format:check": "prettier --check --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-path ../.gitignore",
    "test": "jest -all",
    "pub:beta": "yarn build && npm publish --tag beta",
    "pub:next": "yarn build && npm publish --tag next",
    "pub:release": "yarn build && npm publish --access public"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.253",
    "@types/node-forge": "^1.3.10",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@transia/xrpl": "^2.7.3-alpha.19",
    "node-forge": "^1.3.1",
    "xrpl": "^2.13.0"
  }
}
